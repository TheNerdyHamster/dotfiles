export EDITOR="nvim"
export BROWSER="firefox"
export TERMINAL="alacritty"
export WM="awesome"
export SSH_AUTH_SOCK="$XDG_RUNTIME_DIR/ssh-agent.socket"
export PATH=$PATH:$HOME/.scripts
export PATH=$PATH:$HOME/.config/composer/vendor/bin

if [[ "$(tty)" = "/dev/tty1" ]]; then
  pgrep "$WM" || startx
fi
